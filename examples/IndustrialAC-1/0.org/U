FoamFile
{
    version  2.0;
    format   ascii;
    class    volVectorField;
    location "0";
    object   U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions [0 1 -1 0 0 0 0];
internalField uniform (0 0 0);
boundaryField
{
    cathode
    {
        type noSlip;
    }
    anode
    {
        type noSlip;
    }
    sides
    {
        type pressureInletOutletVelocity;
        value $internalField;
    }
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
