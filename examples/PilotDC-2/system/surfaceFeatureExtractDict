FoamFile
{
    version  2.0;
    format   ascii;
    class    dictionary;
    location "system";
    object   surfaceFeatureExtractDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

modeldomain.stl
{
    extractionMethod extractFromSurface;
    includedAngle 135;
    geometricTestOnly no;
    intersectionMethod none;
    writeObj no;
}

