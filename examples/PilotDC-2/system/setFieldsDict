FoamFile
{
    version  2.0;
    format   ascii;
    class    dictionary;
    location "system";
    object   setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue T 3000
);
regions
(
    cylinderToCell
    {
        p1 (0 0 -100);
        p2 (0 0 100);
        radius 0.007978845608028654;
        fieldValues
        (
            volScalarFieldValue T 10000
        );
    }
);

