FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       fluidLutTransport;
    thermo          fluidLutThermo;
    equationOfState fluidLutEOS;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    specie
    {
        nMoles 1;
        molWeight 1;
    }
    thermodynamics
    {
        #include "dataAir/dataCp.foam";
    }
    transport
    {
        #include "dataAir/dataMu.foam";
        #include "dataAir/dataTk.foam";
        #include "dataAir/dataEk.foam";
    }
    equationOfState
    {
        pRef            101325.;
        #include "dataAir/dataRho.foam";
    }
}

calculateCompositionFieldsYN true;
compositions
{
	#includeIfPresent "dataAir/dataSpecies.foam";
}

