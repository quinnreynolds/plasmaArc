        valueFraction uniform
        (
            1
            0
            0
            0
            0
            1
        );
    }
    top
    {
        type directionMixed;
        refValue uniform (0 0 0);
        refGradient uniform (0 0 0);
        valueFraction uniform
        (
            1
            0
            0
            0
            0
            1
        );
    }
    sides
    {
